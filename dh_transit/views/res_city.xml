<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="res_city_form">
        <field name="name">res.city.form</field>
        <field name="model">res.city</field>
        <field name="arch" type="xml">
            <form string="City">
                <sheet>
                    <group>
                        <field name="nom"/>
                    </group>

                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="res_city_tree">
        <field name="name">res.city.tree</field>
        <field name="model">res.city</field>
        <field name="arch" type="xml">
            <tree string="City">
                <field name="nom"/>
            </tree>
        </field>
    </record>

    <record id="action_res_city" model="ir.actions.act_window">
        <field name="name">City</field>
        <field name="res_model">res.city</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>